    "targetTopic": "Page path string",
    "enclosingTopic": "Project Path Prefix",
    "enclosingSubtopic": "Project Path Prefix"
   },
   {
    "text": ". The project path prefix is specified via the ",
    "type": "text"
   },
   {
    "text": "project path prefix parameter",
    "type": "import",
